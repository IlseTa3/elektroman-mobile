<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp">

    <TextView
        android:id="@+id/text_view_city"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="City"
        android:textColor="@color/darkslateblue"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/text_view_device"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/text_view_city"
        android:text="Device"
        android:textColor="@color/darkslateblue"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/text_view_problem_code"
        android:layout_width="27dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_toEndOf="@+id/text_view_device"
        android:text="Code"
        android:textColor="@color/darkslateblue"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/text_view_customer_name"
        android:layout_width="95dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_toEndOf="@+id/text_view_problem_code"
        android:text="Customer"
        android:textColor="@color/darkslateblue"
        android:textSize="16sp"
        android:textStyle="bold"/>

    <TextView
        android:id="@+id/textview_processed"
        android:layout_width="62dp"
        android:layout_height="44dp"
        android:layout_marginStart="3dp"
        android:layout_toEndOf="@+id/text_view_customer_name"
        android:text="Processed"
        android:textColor="@color/darkslateblue"
        android:textSize="16sp"
        android:textStyle="bold"/>

</RelativeLayout>