<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:padding="5dp">

    <TextView
        android:id="@+id/text_view_city"
        android:layout_width="101dp"
        android:layout_height="wrap_content"
        android:text="City"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/text_view_device"
        android:layout_width="111dp"
        android:layout_height="wrap_content"
        android:layout_toEndOf="@+id/text_view_city"
        android:text="Device"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/text_view_problem_code"
        android:layout_width="27dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="1dp"
        android:layout_toEndOf="@+id/text_view_device"
        android:text="ProblemCode"
        android:textSize="14sp" />

    <TextView
        android:id="@+id/text_view_customer_name"
        android:layout_width="108dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="2dp"
        android:layout_toEndOf="@+id/text_view_problem_code"
        android:text="Customer Name"
        android:textSize="14sp" />

    <CheckBox
        android:id="@+id/checkbox_processed"
        android:layout_width="20dp"
        android:layout_height="20dp"
        android:layout_marginStart="0dp"
        android:layout_toEndOf="@+id/text_view_customer_name"
        />


</RelativeLayout>

